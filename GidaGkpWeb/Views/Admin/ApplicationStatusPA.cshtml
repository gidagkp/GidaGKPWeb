                                        <a style="background-color: green; color: white;" class="btn btn-success" target="_blank"
                                           href="/Admin/DownloadApplicantDocument?applicationId=@UserDataModel.ApplicationId&documentName=Memorendum"><strong>View/Download</strong></a>
                                    }
                                </td>
                                <td class="text-center">
                                    @if (!string.IsNullOrEmpty(UserDataModel.ApplicantDocument.MemorendumFileName))
                                    {
                                        <a><input type="checkbox" /></a>
                                    }
                                </td>
                            </tr>
                            <tr>
                                <td class="text-center">4</td>
                                <td class="text-center">Scan copy of Pan Card</td>
