@using DataLayer;
@using GidaGkpWeb.Models;
@using GidaGkpWeb.Global
@using GidaGkpWeb.Infrastructure.Authentication;
@using GidaGkpWeb.Infrastructure.Utility;
@{

    Layout = "~/Views/Shared/_AdminLayout.cshtml";
    var UserDataModel = ViewData["UserDetail"] as ApplicationUserModel;
}
<link rel="stylesheet" href="~/styles/table_form.css">
<script src="~/Scripts/App/Masters/SchemeWiseApplicationList.js"></script>
<script src="~/Scripts/App/Masters/navigation.js"></script>
<style>
    button.submit_plot_master353 {
        background: #ff2e2e;
        color: #fff;
        border: 0px;
        padding: 3px 10px;
        font-size: 12px;
        text-align: center;
    }

    #DocModal {
        width: 100%;
        left: -185px;
    }

        #DocModal .modal-content {
            width: 160%;
        }

    #basicModal {
        width: 100%;
        left: -185px;
    }

        #basicModal .modal-content {
            width: 160%;
        }
</style>
<div class="container">
    <input type="hidden" value="@UserDataModel.ApplicationId" id="hdnApplicationId" />
    <input type="hidden" value="@UserDataModel.SchemeNameId" id="hdnSchemeNameId" />
    <div class="row">
        <div class="col-sm-12 col-xs-12 text-left" style="padding-bottom: 30px;">
            <h3 style="font-size:18px;">Application Status </h3>
            <hr />
        </div>

        <div class="form-group col-sm-4 col-xs-12">
            <label>Application Number</label>
            <input type="number" class="form-control" placeholder="@UserDataModel.ApplicationNumber" readonly>
        </div>
        <div class="form-group col-sm-4 col-xs-12">
            <label>Applicant Name</label>
            <input type="text" class="form-control" placeholder="@UserDataModel.FullName" readonly>
        </div>
        <div class="form-group col-sm-3 col-xs-12">
            <label>Mobile Number</label>
            <input type="Mobile" class="form-control" placeholder="@UserDataModel.ContactNo" readonly>
        </div>

        @*<div class="form-group col-sm-1 col-xs-12">
                <button type="button" class="submit_plot_master" value="button" style="padding:7px 15px; margin-top: 23px;">Search</button>
            </div>*@
        <div class="col-sm-12 col-xs-12">

            <div class="table-responsive" style="padding:10px 0 80px 0;">
                <table class="table table_css_main_cl table-bordered">
                    <tr>
                        <th class="text-center">S.No.</th>
                        <th class="text-center">Items</th>
                        <th class="text-center">Current Status</th>
                        <th class="text-center">Action</th>
                    </tr>
                    <tr>
                        <td class="text-center">1</td>
                        <td>View Application Details </td>
                        <td></td>
                        <td class="text-right"> <button type="button" data-toggle="modal" data-target="#myModal" class="submit_plot_master353" value="button">View Details </button></td>
                    </tr>
                    <tr>
                        <td class="text-center">2</td>
                        <td>View Financial Status & Verify </td>
                        <td>
                            @if (UserDataModel.AMPaymentStatus != null && UserDataModel.AMPaymentStatus.Contains("Not Approved"))
                            {
                                <span style="color:red">Assistant Manager - @UserDataModel.AMPaymentStatus</span>
                            }
                            else if (UserDataModel.AMPaymentStatus != null && UserDataModel.AMPaymentStatus.Contains("Approved"))
                            {
                                <span style="color:green">Assistant Manager - @UserDataModel.AMPaymentStatus</span>
                            }
                            <br />
                            @if (UserDataModel.MPaymentStatus != null && UserDataModel.MPaymentStatus.Contains("Not Approved"))
                            {
                                <span style="color:red">Manager - @UserDataModel.MPaymentStatus</span>
                            }
                            else if (UserDataModel.MPaymentStatus != null && UserDataModel.MPaymentStatus.Contains("Approved"))
                            {
                                <span style="color:green">Manager - @UserDataModel.MPaymentStatus</span>
                            }
                            <br />
                            @if (UserDataModel.GMPaymentStatus != null && UserDataModel.GMPaymentStatus.Contains("Not Approved"))
                            {
                                <span style="color:red">General Manager - @UserDataModel.GMPaymentStatus   </span>
                            }
                            else if (UserDataModel.GMPaymentStatus != null && UserDataModel.GMPaymentStatus.Contains("Approved"))
                            {
                                <span style="color:green">General Manager - @UserDataModel.GMPaymentStatus</span>
                            }
                        </td>
                        <td class="text-right">
                            @if (hasPermissionOnPage("Scheme Wise Applicant List - Application Status - Finance Approval")
                                && hasPermission(PermissionName.AllAccess))
                            {
                                if (UserData.Department == "Finance & Accounts" && UserData.Designation == "Assistant Manager")
                                {
                                    if (string.IsNullOrEmpty(UserDataModel.AMPaymentStatus))
                                    {
                                        <button type="button" data-toggle="modal" data-target="#basicModal" class="submit_plot_master353" value="button">Financial Details </button>
                                    }
                                    else
                                    {
                                        <button type="button" class="submit_plot_master353" value="button" disabled="disabled" style="opacity: 0.5;">Financial Details </button>
                                    }
                                }
                                else if (UserData.Department == "Finance & Accounts" && UserData.Designation == "Manager")
                                {
                                    if (!string.IsNullOrEmpty(UserDataModel.AMPaymentStatus) && string.IsNullOrEmpty(UserDataModel.MPaymentStatus))
                                    {
                                        <button type="button" data-toggle="modal" data-target="#basicModal" class="submit_plot_master353" value="button">Financial Details </button>
                                    }
                                    else
                                    {
                                        <button type="button" class="submit_plot_master353" value="button" disabled="disabled" style="opacity: 0.5;">Financial Details </button>
                                    }
                                }
                                else if (UserData.Department == "Finance & Accounts" && UserData.Designation == "General Manager")
                                {
                                    if (!string.IsNullOrEmpty(UserDataModel.AMPaymentStatus) && !string.IsNullOrEmpty(UserDataModel.MPaymentStatus) && string.IsNullOrEmpty(UserDataModel.GMPaymentStatus))
                                    {
                                        <button type="button" data-toggle="modal" data-target="#basicModal" class="submit_plot_master353" value="button">Financial Details </button>
                                    }
                                    else
                                    {
                                        <button type="button" class="submit_plot_master353" value="button" disabled="disabled" style="opacity: 0.5;">Financial Details </button>
                                    }
                                }
                            }
                        </td>
                    </tr>
                    <tr>
                        <td class="text-center">3</td>
                        <td>View Uploaded Documents Status & Verify </td>
                        <td>
                            @if (UserDataModel.ClerkDocumentStatus != null && UserDataModel.ClerkDocumentStatus.Contains("Not Approved"))
                            {
                                <span style="color:red">Clerk - @UserDataModel.ClerkDocumentStatus</span>
                            }
                            else if (UserDataModel.ClerkDocumentStatus != null && UserDataModel.ClerkDocumentStatus.Contains("Approved"))
                            {
                                <span style="color:green">Clerk - @UserDataModel.ClerkDocumentStatus</span>
                            }
                            <br />
                            @if (UserDataModel.AMDocumentStatus != null && UserDataModel.AMDocumentStatus.Contains("Not Approved"))
                            {
                                <span style="color:red">Assistant Manager - @UserDataModel.AMDocumentStatus</span>
                            }
                            else if (UserDataModel.AMDocumentStatus != null && UserDataModel.AMDocumentStatus.Contains("Approved"))
                            {
                                <span style="color:green">Assistant Manager - @UserDataModel.AMDocumentStatus</span>
                            }
                            <br />
                            @if (UserDataModel.SIDocumentStatus != null && UserDataModel.SIDocumentStatus.Contains("Not Approved"))
                            {
                                <span style="color:red">Section Incharge - @UserDataModel.SIDocumentStatus</span>
                            }
                            else if (UserDataModel.SIDocumentStatus != null && UserDataModel.SIDocumentStatus.Contains("Approved"))
                            {
                                <span style="color:green">Section Incharge - @UserDataModel.SIDocumentStatus</span>
                            }
                        </td>
                        <td class="text-right">
                            @if (hasPermissionOnPage("Scheme Wise Applicant List - Application Status - Document Approval")
                               && hasPermission(PermissionName.AllAccess))
                            {
                                if (UserData.Department == "Property" && UserData.Designation == "Clerk")
                                {
                                    if (string.IsNullOrEmpty(UserDataModel.ClerkDocumentStatus))
                                    {
                                        <button type="button" data-toggle="modal" data-target="#DocModal" class="submit_plot_master353" value="button">Check Documents </button>
                                    }
                                    else
                                    {
                                        <button type="button" class="submit_plot_master353" value="button" disabled="disabled" style="opacity: 0.5;">Check Documents </button>
                                    }
                                }
                                else if (UserData.Department == "Property" && UserData.Designation == "Assistant Manager")
                                {
                                    if (!string.IsNullOrEmpty(UserDataModel.ClerkDocumentStatus) && string.IsNullOrEmpty(UserDataModel.AMDocumentStatus))
                                    {
                                        <button type="button" data-toggle="modal" data-target="#DocModal" class="submit_plot_master353" value="button">Check Documents </button>
                                    }
                                    else
                                    {
                                        <button type="button" class="submit_plot_master353" value="button" disabled="disabled" style="opacity: 0.5;">Check Documents </button>
                                    }
                                }
                                else if (UserData.Department == "Property" && UserData.Designation == "Section Incharge")
                                {
                                    if (!string.IsNullOrEmpty(UserDataModel.ClerkDocumentStatus) && !string.IsNullOrEmpty(UserDataModel.AMDocumentStatus) && string.IsNullOrEmpty(UserDataModel.SIDocumentStatus))
                                    {
                                        <button type="button" data-toggle="modal" data-target="#DocModal" class="submit_plot_master353" value="button">Check Documents </button>
                                    }
                                    else
                                    {
                                        <button type="button" class="submit_plot_master353" value="button" disabled="disabled" style="opacity: 0.5;">Check Documents </button>
                                    }
                                }
                            }
                        </td>
                    </tr>
                </table>
            </div>
        </div>
    </div>

</div>
<div class="modal fade" id="myModal" role="dialog">
    <div class="modal-dialog modal-lg">
        <div class="modal-content">
            <div class="modal-header">
                <button type="button" class="close" data-dismiss="modal">&times;</button>
                <h4 class="modal-title">Application Details</h4>
            </div>
            <div class="modal-body">

                <div class="row">
                    <div class="form-group col-sm-6 col-xs-12">
                        <label for="user" class="">User Name</label>
                        <input type="text" class="form-control" placeholder="@UserDataModel.UserName">

                    </div>
                    <div class="form-group col-sm-6 col-xs-12">
                        <label for="user" class="">Applicant Name</label>
                        <input type="text" class="form-control" placeholder="@UserDataModel.FullName">
                    </div>
                    <div class="form-group col-sm-6 col-xs-12">
                        <label for="user" class="">Father Name</label>
                        <input type="text" class="form-control" placeholder="@UserDataModel.FatherName">
                    </div>
                    <div class="form-group col-sm-6 col-xs-12">
                        <label for="user" class="">Application Number</label>
                        <input type="text" class="form-control" placeholder="@UserDataModel.ApplicationNumber">
                    </div>
                    <div class="form-group col-sm-6 col-xs-12">
                        <label for="user" class="">Plot Area</label>
                        <input type="text" class="form-control" placeholder="@UserDataModel.PlotArea">
                    </div>
                    <div class="form-group col-sm-6 col-xs-12">
                        <label for="user" class="">Amount Paid(In Rs.)</label>
                        <input type="text" class="form-control" placeholder="@UserDataModel.PaidAmount">
                    </div>
                    <div class="form-group col-sm-6 col-xs-12">
                        <label for="user" class="">Contact Number</label>
                        <input type="text" class="form-control" placeholder="@UserDataModel.ContactNo">
                    </div>
                    <div class="form-group col-sm-6 col-xs-12">
                        <label for="user" class="">Email</label>
                        <input type="text" class="form-control" placeholder="@UserDataModel.Email">
                    </div>
                    <div class="form-group col-sm-6 col-xs-12">
                        <label for="user" class="">Aadhar Number</label>
                        <input type="text" class="form-control" placeholder="@UserDataModel.AadharNumber">
                    </div>
                    <div class="form-group col-sm-6 col-xs-12">
                        <label for="user" class="">D.O.B.</label>
                        <input type="text" class="form-control" placeholder="@UserDataModel.DOB">
                    </div>

                    @*<div class="form-group col-sm-6 col-xs-12">
                            <label for="user" class="">Comapny</label>
                            <input type="text" class="form-control" placeholder="@UserDataModel.com">
                        </div>*@
                    <div class="form-group col-sm-6 col-xs-12">
                        <label for="user" class="">Total Investment Amount(In Rs.)</label>
                        <input type="text" class="form-control" placeholder="@UserDataModel.TotalInvestment">
                    </div>
                    @*<div class="form-group col-sm-6 col-xs-12">
                            <label for="user" class="">Employement</label>
                            <input type="text" class="form-control" placeholder="@UserDataModel.emp">
                        </div>*@
                    <div class="form-group col-sm-12 col-xs-12">
                        <label for="user" class="">Current Address</label>
                        <input type="text" class="form-control" placeholder="@UserDataModel.CAddress">
                    </div>


                    <div class="col-sm-12 col-xs-12" style="padding:5px 15px 10px 15px; margin-bottom: 10px; border-bottom:1px solid #f5f5f5;">
                        <h3 style="font-size:15px;">Payment Details</h3>
                    </div>


                    <div class="form-group col-sm-6 col-xs-12">
                        <label for="user" class="">Bank Name</label>
                        <input type="text" class="form-control" placeholder="@UserDataModel.BankName">
                    </div>
                    <div class="form-group col-sm-6 col-xs-12">
                        <label for="user" class="">Payment Mode</label>
                        <input type="text" class="form-control" placeholder="@UserDataModel.PaymentMode">
                    </div>
                    <div class="form-group col-sm-6 col-xs-12">
                        <label for="user" class="">Transaction ID</label>
                        <input type="text" class="form-control" placeholder="@UserDataModel.TransactionID">
                    </div>
                    <div class="form-group col-sm-6 col-xs-12">
                        <label for="user" class="">Payment  Date</label>
                        <input type="text" class="form-control" placeholder="@UserDataModel.TransactionDate">
                    </div>
                </div>
            </div>
            <div class="modal-footer">
                <!--<button type="button" class="btn btn-default" style="background:rgb(194, 63, 63); color:#fff;">Close</button>-->
                <button type="button" class="btn btn-default" data-dismiss="modal">Close</button>
            </div>

        </div>
    </div>
</div>

