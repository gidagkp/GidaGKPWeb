                                        <a style="background-color: green; color: white;" class="btn btn-success" target="_blank"
                                           href="/Admin/DownloadApplicantDocument?applicationId=@UserDataModel.ApplicationId&documentName=ScanAddressProof"><strong>View/Download</strong></a>
                                    }
                                </td>
                                <td class="text-center">
                                    @if (!string.IsNullOrEmpty(UserDataModel.ApplicantDocument.ScanAddressProofFileName))
                                    {
                                        <a><input type="checkbox" /></a>
                                    }
                                </td>
                            <tr>
