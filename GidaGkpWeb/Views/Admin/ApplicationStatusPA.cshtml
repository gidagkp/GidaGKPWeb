<div class="modal fade" id="DocModal" tabindex="-1" role="dialog" aria-labelledby="basicModal" aria-hidden="true">
    <div class="modal-dialog">
        <div class="modal-content">
            <div class="modal-header">
                <button type="button" class="close" data-dismiss="modal" aria-hidden="true">&times;</button>
                <h4 class="modal-title" id="myModalLabel">CheckList Document Verify</h4>
            </div>
            <div class="modal-body pull-left" style="width:100%;">
                <div class="col-sm-12 col-xs-12">

                    <div class="table-responsive" style="padding:10px 0 0px 0;">
                        <table class="table table_css_main_cl table-bordered">

                            <tr>
                                <th class="text-center">S.No.</th>
                                <th class="text-center">CheckList Document Name</th>
                                <th class="text-center">View/Download</th>
                                <th class="text-center">Verify</th>
                            </tr>
                            <tr>
                                <td class="text-center">1</td>
                                <td class="text-center">Project Reports</td>
                                <td class="text-center">
                                    @if (!string.IsNullOrEmpty(UserDataModel.ApplicantDocument.ProjectReportFileName))
                                    {
                                        <a style="background-color: green; color: white;" class="btn btn-success" target="_blank"
                                           href="/Admin/DownloadApplicantDocument?applicationId=@UserDataModel.ApplicationId&documentName=ProjectReport"><strong>View/Download</strong></a>
                                    }
                                </td>
                                <td class="text-center">
                                    @if (!string.IsNullOrEmpty(UserDataModel.ApplicantDocument.ProjectReportFileName))
                                    {
                                        <a><input type="checkbox" /></a>
                                    }
                                </td>
                            </tr>
                            <tr>
                                <td class="text-center">2</td>
                                <td class="text-center">Proposed plan of land usage</td>
