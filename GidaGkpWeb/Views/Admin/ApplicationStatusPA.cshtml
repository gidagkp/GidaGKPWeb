                                           href="/Admin/DownloadApplicantDocument?applicationId=@UserDataModel.ApplicationId&documentName=ProposedPlanLandUses"><strong>View/Download</strong></a>
                                    }

                                </td>
                                <td class="text-center">
                                    @if (!string.IsNullOrEmpty(UserDataModel.ApplicantDocument.ProposedPlanLandUsesFileName))
