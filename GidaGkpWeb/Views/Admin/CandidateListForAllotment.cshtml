@using DataLayer;
@using GidaGkpWeb.Models;
@{

    Layout = "~/Views/Shared/_AdminLayout.cshtml";
    var UserData = ViewData["PageData"] as List<PageMasterModel>;

}

<link rel="stylesheet" href="~/styles/table_form.css">
<script src="~/Scripts/App/Masters/PlotMaster.js"></script>
<script src="~/Scripts/App/Masters/navigation.js"></script>
<div class="container">
    <div class="col-sm-12 col-xs-12" style="padding:10px 0 10px 0; margin-bottom: 50px; border-bottom:1px solid rgb(170, 170, 170);">
