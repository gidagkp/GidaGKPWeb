@using DataLayer;
@using GidaGkpWeb.Models;

    @{
        ViewBag.Title = "Leased Duty Work";
        Layout = "~/Views/Shared/_AdminLayout.cshtml";

        var UserData = ViewData["UserData"] as List<GidaUserModel>;
        int index = 1;
    }
    <link rel="stylesheet" href="~/styles/table_form.css">
    <script src="~/Scripts/App/Masters/AdminLookup.js"></script>
    <script src="~/Scripts/App/Masters/navigation.js"></script>

    <div class="container">
        <form action="~/Admin/SaveNewLeasedDutyWork" method="post" enctype="multipart/form-data">

            <div class="row">
                <div class="col-sm-12 col-xs-12">
                    <h3 style="font-size:18px;">Seeking the response for Lease Duty Work     </h3>
                    <hr />
                </div>



                <div class="col-sm-12 col-xs-12">

                    <div class="table-responsive" style="padding:10px 0 30px 0;">
                        <table class="table table_css_main_cl table-bordered">

                            <tr>
                                <th class="text-center">S.No.</th>
                                <th class="text-center">Items</th>
                                <th class="text-center">Action</th>

                            </tr>

                            <tr>
                                <td class="text-center">1</td>
                                <td>Stamp Value Required for the lease to happen based on the Allotment Master Data </td>
                                <td class="text-right">
                                    <div class="form-group col-sm-12 col-xs-12">
                                        <input type="number" class="form-control" placeholder="50000">
                                        <input type="number" name="AllotmentMoneyPaid" id="AllotmentMoneyPaid" class="form-control">
                                    </div>
                                </td>
                            </tr>

                            <tr>
                                <td class="text-center">2</td>
                                <td>Bank Guarantee Challan Number </td>
                                <td class="text-right">
                                    <div class="form-group col-sm-12 col-xs-12">
                                        <input type="text" name="BankGauranteeChallanNo" id="BankGauranteeChallanNo" class="form-control" placeholder="">
                                    </div>
                                </td>
                            </tr>
                            <tr>
                                <td class="text-center">3</td>
                                <td>Value of the Stamp Purchased</td>
                                <td class="text-right">
                                    <div class="form-group col-sm-12 col-xs-12">
                                        <input type="number" name="PurchasedStampValue" id="PurchasedStampValue" class="form-control" placeholder="">
                                    </div>
                                </td>
                            </tr>
                            <tr>
                                <td class="text-center">4</td>
                                <td>Stamp Value Bank Guarantee Challan Number </td>
                                <td class="text-right">
                                    <div class="form-group col-sm-12 col-xs-12">
                                        <input type="number" name="StampValueForBankGaurantee" id="StampValueForBankGaurantee" class="form-control" placeholder="">
                                    </div>
                                </td>
                            </tr>
                            <tr>
                                <td class="text-center">5</td>
                                <td>Date of the bank guarantee for the stamp value. </td>
                                <td class="text-right">
                                    <div class="form-group col-sm-12 col-xs-12">
                                        <input type="date" name="BankGauranteeDate" id="BankGauranteeDate" class="form-control" placeholder="">
                                    </div>
                                </td>
                            </tr>

                            <tr>
                                <td class="text-center">6</td>
                                <td>Name of the entity on behalf of the Applicant, which has purchased the stamp papers and took the bank guarantee. </td>
                                <td class="text-right">
                                    <div class="form-group col-sm-12 col-xs-12">
                                        <input type="text" id="EntityNameBehalfOfApplicant" name="EntityNameBehalfOfApplicant" class="form-control" placeholder="">
                                    </div>
                                </td>
                            </tr>



                        </table>
                    </div>
                </div>



                <div class="col-sm-12 col-xs-12" style="padding:20px 15px 50px 15px;">
                    <input type="hidden" name="Id" value="" />
                    <input type="hidden" name="ApplicationId" value="1234567" />
                   
                    <input type="submit" id="btnSave" name="next" class="submit_plot_master" value="Send" />
                    <button type="button" class="cancel_plot_master" value="button">Cancel</button>

                </div>

            </div>
</form> 
    </div>


